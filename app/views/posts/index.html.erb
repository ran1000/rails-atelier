<%= render "shared/navbar" %>
<%= render "shared/footer" %>
<% @posts.each do |post| %>
  <%= render "shared/card", post: post %>
<% end %>
<% tags = ["art", "inspiration", "notes", "projects", "todigital"] %>
<% tags.each do |tag| %>
<div>
<%= link_to "#{tag.capitalize}", about_path(tag: tag) %>
</div>
<% end %>
